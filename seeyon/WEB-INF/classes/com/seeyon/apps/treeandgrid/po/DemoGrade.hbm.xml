<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.seeyon.apps.treeandgrid.po">
  <class
    name="DemoGrade"
    table="demo_grades"
  >
    <id
      name="id"
      type="long"
      column="ID"
    >
      <generator class="assigned"/>
    </id>
    
        <!-- 标题 -->
    <property
      name="subject"
      column="SUBJECT"
      type="string"
      not-null="false"
      length="50"
    />

    <property
      name="stype"
      column="stype"
      type="string"
      not-null="false"
      length="50"
    />

    <property
      name="parentId"
      column="pid"
      type="long"
      not-null="false"
      length="22"
    />

  </class>
</hibernate-mapping>
