<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.seeyon.apps.treeandgrid.po">
    <!-- 个人事项 -->
  <class
    name="DemoStudent"
    table="demo_student"
  >
    <id
      name="id"
      type="long"
      column="ID"
    >
      <generator class="assigned"/>
    </id>

    <property
      name="subject"
      column="subject"
      type="string"
      not-null="false"
      length="7"
    />

    <property
      name="grades"
      column="grades"
      type="string"
      not-null="false"
      length="255"
    />
    <!-- 流程CaseID -->
    <property
      name="sex"
      column="sex"
      type="string"
      not-null="false"
      length="10"
    />
     <!-- 单位ID -->
    <property
      name="age"
      column="age"
      type="string"
      not-null="false"
      length="10"
    />

  </class>
</hibernate-mapping>
